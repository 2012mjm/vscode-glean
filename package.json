{
    "name": "export-to-file",
    "displayName": "export-to-file",
    "description": "",
    "version": "0.0.1",
    "publisher": "wix",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.extractToFile",
                "title": "Extract to File"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Extract to File Configuration",
            "properties": {
                "extract-to-file.jsFilesExtentions": {
                    "type": [
                        "string"
                    ],
                    "default": [
                        "js",
                        "jsx",
                        "ts",
                        "tsx"
                    ],
                    "description": "File extentions to be treated as Javascript files"
                },
                "extract-to-file.jsModuleSystem": {
                    "type": "string",
                    "default": "esm",
                    "description": "Module system to be used whem extracting Javascript"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/chai": "^4.1.2",
        "@types/mocha": "^2.2.48",
        "@types/node": "^7.0.43",
        "@types/sinon": "^4.3.0",
        "@types/sinon-chai": "^2.7.29",
        "chai": "^4.1.2",
        "line-column": "^1.0.2",
        "sinon": "^4.5.0",
        "sinon-chai": "^3.0.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "@babel/core": "^7.0.0-beta.40",
        "@babel/traverse": "^7.0.0-beta.40",
        "@babel/types": "^7.0.0-beta.40",
        "babylon": "^7.0.0-beta.40",
        "gitignore-globs": "^0.1.1",
        "gitignore-to-glob": "^0.3.0",
        "mkdirp": "^0.5.1",
        "prepend": "^1.0.2",
        "prepend-file": "^1.3.1",
        "relative": "^3.0.2",
        "vscode-cache": "^0.3.0"
    }
}
